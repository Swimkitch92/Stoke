{
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

	"name": "Stoke",
	"description": "Gives you the best time to surf. Period.",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
	"version": "0.0.1",
	"permissions": [
    "storage", "tabs", "<all_urls>"
  ]
}
